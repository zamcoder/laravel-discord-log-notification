{
    "name": "zamcoder/laravel-discord-logger",
    "description": "Logging to a discord channel in Laravel",
    "keywords": [
        "laravel",
        "discord",
        "logging",
        "logger"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Edwin Zamzami",
        "email": "edwinzamzami01@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.3 | ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ZamCoder\\DiscordLogger\\": "src/DiscordLogger"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZamCoder\\DiscordLogger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZamCoder\\DiscordLogger\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}